{
	"manifest_version": 2,
	"name": "Veto Me",
	"description": "Chrome Extension for to Veto songs on Play Me",
	"version": "0.0.1.3",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"activeTab",
		"cookies",
		"http://*/*",
		"https://*/*"
	]
}